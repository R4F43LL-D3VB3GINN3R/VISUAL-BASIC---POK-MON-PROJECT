Public Class MessageBoxRedTeam
    ' Classe que representa uma caixa de mensagem para a equipe vermelha

    Private Sub Button1_Click(sender As Object, e As EventArgs) Handles Button1.Click
        ' Manipula o evento de clique no botão "Button1"

        ' Esconde a caixa de mensagem atual
        Me.Hide()

        ' Exibe o Formulário 1
        Form1.Show()
    End Sub
End Class
